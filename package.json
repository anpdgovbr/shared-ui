{
  "name": "@anpdgovbr/shared-ui",
  "version": "0.2.0",
  "description": "Biblioteca de componentes React com MUI v7 e padrão govbr-ds para ANPD",
  "keywords": [
    "react",
    "typescript",
    "components",
    "ui",
    "govbr",
    "govbr-ds",
    "design-system",
    "mui",
    "material-ui",
    "anpd",
    "governo",
    "brasil",
    "acessibilidade",
    "a11y"
  ],
  "author": {
    "name": "Luciano Édipo",
    "email": "luciano.psilva@anpd.gov.br",
    "url": "https://github.com/lucianoedipo"
  },
  "contributors": [
    {
      "name": "Equipe ANPD",
      "url": "https://github.com/orgs/anpdgovbr/people"
    }
  ],
  "license": "ISC",
  "type": "module",
  "main": "./dist/shared-ui.cjs.js",
  "module": "./dist/shared-ui.es.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/src/index.d.ts",
      "import": "./dist/shared-ui.es.js",
      "require": "./dist/shared-ui.cjs.js"
    },
    "./theme": {
      "types": "./types/src/theme/govbrTheme.d.ts",
      "import": "./dist/shared-ui.es.js",
      "require": "./dist/shared-ui.cjs.js"
    },
    "./theme-provider": {
      "types": "./types/src/theme/GovBRThemeProvider.d.ts",
      "import": "./dist/shared-ui.es.js",
      "require": "./dist/shared-ui.cjs.js"
    },
    "./styles": "./dist/shared-ui.css"
  },
  "files": [
    "dist",
    "types",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "npm run storybook",
    "build": "npm run build:types && vite build",
    "build:types": "tsc --emitDeclarationOnly --outDir types",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "check": "npm run lint && npm run type-check",
    "prepublishOnly": "npm run build && npm run check",
    "publish:beta": "npm publish --tag beta",
    "version:beta": "npm version prerelease --preid=beta",
    "version:beta-patch": "npm version prerelease --preid=beta",
    "version:beta-minor": "npm version preminor --preid=beta",
    "version:beta-major": "npm version premajor --preid=beta",
    "release:beta": "npm run version:beta && npm run publish:beta"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anpdgovbr/shared-ui.git"
  },
  "bugs": {
    "url": "https://github.com/anpdgovbr/shared-ui/issues",
    "email": "ti@anpd.gov.br"
  },
  "homepage": "https://github.com/anpdgovbr/shared-ui#readme",
  "funding": {
    "type": "government",
    "url": "https://anpd.gov.br"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "peerDependencies": {
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.45.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@govbr-ds/core": "^3.6.2",
    "classnames": "^2.5.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.0",
    "@eslint/js": "^9.33.0",
    "@hookform/resolvers": "^5.2.1",
    "@storybook/addon-a11y": "^9.1.1",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-onboarding": "^9.1.1",
    "@storybook/addon-vitest": "^9.1.1",
    "@storybook/react-vite": "^9.1.1",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.1.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "storybook": "^9.1.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "zod": "^4.0.17"
  }
}
